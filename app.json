{
  "expo": {
    "name": "Zenduja",
    "slug": "Zenduja-Pro",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#222222"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns":[
      "*.*",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.zenduja.game"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "png"
      ]
    }
  }
}
